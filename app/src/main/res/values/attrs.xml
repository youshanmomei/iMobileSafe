<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="SettingItemView">

        <!--display different title content-->
        <attr name="settingItemTitle" format="string" />

        <!--setting background-->
        <attr name="itbackground">
            <enum name="first" value="0" />
            <enum name="middle" value="1" />
            <enum name="last" value="2"/>
        </attr>

        <!--if the switch is not visiable-->
        <attr name="toggleEnable" format="boolean" />

    </declare-styleable>
</resources>